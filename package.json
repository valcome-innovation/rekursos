  "description": "Rekursos: Titan of TypeScript üî±üèõÔ∏è A simple TypeScript library providing essential utility functions, helpers, and operators.",
  "scripts": {
    "test": "jest",
    "build": "tsc --project tsconfig.esm.json & tsc --project tsconfig.cjs.json",
    "barrels": "barrelsby --directory src --delete --singleQuotes --noHeader -e \".*\\\\.spec\\\\.ts\""
  },
  "private": false,
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
