  "optionalDependencies": {
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "barrelsby": "^2.8.1",
